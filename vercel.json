{
  "functions": {
    "app/api/process-pdf/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "pdf_inverter.py",
      "use": "@vercel/python"
    }
  ],
  "headers": [
    {
      "source": "/api/process-pdf",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
